# Copy this file to .env (do NOT commit .env)
# Firebase (safe to expose in client)
EXPO_PUBLIC_API_KEY="YOUR_FIREBASE_API_KEY"
EXPO_PUBLIC_AUTH_DOMAIN="YOUR_PROJECT.firebaseapp.com"
EXPO_PUBLIC_PROJECT_ID="YOUR_PROJECT_ID"
EXPO_PUBLIC_STORAGE_BUCKET="YOUR_PROJECT.appspot.com"
EXPO_PUBLIC_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID"
EXPO_PUBLIC_APP_ID="YOUR_APP_ID"
EXPO_PUBLIC_MEASUREMENT_ID="YOUR_MEASUREMENT_ID"

# AI proxy endpoint (use a serverless function; do not put AI keys in the client)
EXPO_PUBLIC_AI_PROXY_URL="https://your-domain.example.com/api"
